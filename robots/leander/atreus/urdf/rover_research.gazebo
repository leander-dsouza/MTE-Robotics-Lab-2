<!--......................MULTI- DIFF DRIVE (HECTOR)..........................................-->


<gazebo>
  <plugin name="diffdrive_plugin_multiwheel" filename="libdiffdrive_plugin_multi_wheel.so">
    <updateRate>50.0</updateRate>
    <leftJoints>base_to_left_front_leg base_to_left_middle_leg base_to_left_leg </leftJoints>
    <rightJoints>base_to_right_front_leg base_to_right_middle_leg base_to_right_leg </rightJoints>
    <wheelSeparation>0.6</wheelSeparation>
    <wheelDiameter>0.2</wheelDiameter>
    <torque>30</torque>
    <commandTopic>cmd_vel</commandTopic>
    <odometryTopic>odom</odometryTopic>
    <odometryFrame>odom</odometryFrame>
    <robotBaseFrame>base_link</robotBaseFrame>
    <publishOdomTF>true</publishOdomTF>
    <robotBaseFrame>base_link</robotBaseFrame>
    <publishWheelTF>false</publishWheelTF>
    <publishWheelJointState>false</publishWheelJointState>
    <rosDebugLevel>na</rosDebugLevel>
    <wheelAcceleration>0</wheelAcceleration>
    <wheelTorque>5</wheelTorque>
    <odometrySource>world</odometrySource>
    <publishTf>1</publishTf>
  </plugin>
</gazebo>

